@model TicariOtomasyon.Models.Entities.Departman
@{
    ViewData["Title"] = "DepartmanEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h1>Departman Ekle</h1>
<br />

<form class="form-group" method="post">
    @Html.Label("Departman Adı:")
    @Html.TextBoxFor(x => x.DepartmanAdi, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Ekle</button>
</form>
